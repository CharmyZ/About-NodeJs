<% include header %>
<% include menu %>
<div class="index-main">
    <div class="table">
        <ul class="ul-table table-header">
            <li>昵称</li>
            <li>用户名</li>
            <li>密码</li>
            <li>性别</li>
            <li>年龄</li>
            <li>生日</li>
            <li>描述</li>
        </ul>
        <% for(var i = 0, l = list.length;i < l;i++){ %>
        <ul class="ul-table table-body">
            <li><%= list[i].nickname %></li>
            <li><%= list[i].username %></li>
            <li><%= getPassword(list[i].password)%></li>
            <li><%= getEmpty(list[i].sex) %></li>
            <li><%= getEmpty(list[i].age) %></li>
            <li><%= getEmpty(list[i].birthday) %></li>
            <li><%= getEmpty(list[i].desc) %></li>
            <li><a href="javascript:;" class="blue-btn">查看</a></li>
        </ul>
        <% } %>
    </div>
</div>
<% include footer %>

